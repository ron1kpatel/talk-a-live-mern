# MongoDB connection URI (include <password> placeholder if using a full URI)
MONGO_URI=mongodb+srv://<username>:<password>@cluster0.<cluster-id>.mongodb.net/

# MongoDB password (optional if included in URI)
MONGODB_PASSWORD=your_mongodb_password

# Server port
PORT=8080

# Environment type: development | production
NODE_ENV=development

# Secret key for JWT signing
JWT_SECRET=your_jwt_secret_key
